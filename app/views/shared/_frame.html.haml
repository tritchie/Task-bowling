-if @frame.id < 11
  %td.frame
    %table
      %tr
        %td.id
          = @frame.id
        %td.box1
          = @frame.ball1
        %td.box2
          = @frame.ball2
      %tr
        %td.total{:colspan => 2}
          = @frame.total
      %tr
        %td.extras{:colspan => 2}
          = link_to 'Edit', edit_frame_path(@frame)
          
