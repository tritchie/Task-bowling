%td{frameattrs}
  -if active?
    %table
      %tr
        =render :partial => 'frames/activeframe'
        =bonus if @frame.position == 10
      %tr
        %td 1st
        %td 2nd
  -else
    %table
      %tr
        =render :partial => 'frames/inactiveframe'
        =bonus if @frame.position == 10
      %tr
        %td.total{:colspan => 2}
          = total
