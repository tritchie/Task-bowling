-#(:url => {:controller => 'games', :action => 'update', :id => @game.id})}
%td.frame
  -#{:onclick => (:url => {:controller => 'games', :action => 'update', :game => {:current_frame => @frame.id}})}
  %a{:href => "/games/#{@game.id}/update/#{@frame.id}"}
    %table
      %tr
        %td.box1
          = boxes[0]
        %td.box2
          = boxes[1]
      %tr
        %td.total{:colspan => 2}
          = @frame.total
      %tr
        %td.extras{:colspan => 2}
          = link_to 'Edit', edit_frame_path(@frame)
